<%= render partial: 'semesters/semester', locals: {semester: @semester, main: true} %>

<%= content_for :prefetches do %>
	<% @sections.each do |section| %>
		<link rel="prefetch" href="<%= semester_section_assignments_url(@semester, section) %>">
