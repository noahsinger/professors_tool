<div class="block assignments header <%= double_width_if_main(defined? main) %>">
	<header>
		<%= link_to semester_section_assignments_path(section.semester,section) do %>
			<h4>Assignments</h4>
		<% end %>
	</header>
	<nav>
		<ul>
			<li><%= link_to( 'RSS' , semester_section_assignments_path(section.semester,section, format: :rss), class: 'icon rss' ) %></li>
			<li><%= link_to( 'ICAL', semester_section_assignments_url(section.semester,section, format: :ics, protocol: 'webcal'), title: 'Subscribe to this classes assignments calendar using Outlook or iCal', class: 'icon ical' ) %></li>
		</ul>
	</nav>
</div>
