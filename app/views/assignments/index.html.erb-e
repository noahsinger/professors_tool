<%= render 'semesters/semesters_header' %>
<%= render partial: 'semesters/semester', locals: xxx@xxx.xxx %>
<%= render partial: 'sections/section', locals: xxx@xxx.xxx main: true} %>

<% xxx@xxx.xxx > 0 %>
	<%= render partial: 'materials/materials_header', locals: xxx@xxx.xxx %>
<% end %>

<% xxx@xxx.xxx > 0 %>
	<%= render partial: 'examples/examples_header', locals: {semester: @semester, xxx@xxx.xxx %>
<% end %>

<% if current_user %>
	<div class="block">
		<header>
			<h1>Admin</h1>
		</header>
		<nav>
			<ul>
				<li><%= link_to "Section Admin", xxx@xxx.xxx %></li>
			</ul>
		</nav>
	</div>
<% end %>

<% xxx@xxx.xxx > 0 %>
	<div class="block">
		<header>
			<h3>Tutorials</h3>
		</header>
		<ul>
xxx@xxx.xxx do |howto| %>
				<li><%= link_to howto.title, course_how_to_step_path(@section.course, howto, howto.steps.first, xxx@xxx.xxx %></li>
			<% end %>
		</ul>
	</div>
<% end %>

<% xxx@xxx.xxx > 0 %>
xxx@xxx.xxx do |assignment| %>
		<%= render assignment %>
	<% end %>
<% else %>
	<%= render partial: 'no_assignments', locals: xxx@xxx.xxx %>
<% end %>
