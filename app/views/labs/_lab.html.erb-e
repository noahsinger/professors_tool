<div class="block labs <%= double_width_if_main(defined? main) %>">
	<header>
		<%= link_to course_lab_path(lab.course, lab) do %>
			<p><%= pluralize lab.worth, 'point' %></p>
			<h3><%= lab.title %></h3>
		<% end %>
	</header>
	
	<ul id="assignment_links">
		<li><a href="#" id="print" class="icon file">Print this Lab</a></li>
		
		<% if current_user %>
			<li><%= link_to 'Edit this Lab', edit_admin_course_lab_path(lab.course, lab) %></li>
		<% end %>			
	</ul>
</div>
