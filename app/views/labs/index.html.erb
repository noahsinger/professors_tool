<h1>Listing Labs for xxx@xxx.xxx %></h1>

<table>
  <tr>
    <th>Course</th>
    <th>Title</th>
    <th>Objective</th>
    <th>Instructions</th>
  </tr>

<% for lab xxx@xxx.xxx %>
  <tr>
    <td><%=h lab.course.title %></td>
    <td><%=h lab.title %></td>
    <td><%=h lab.objective %></td>
    <td><%=h lab.instructions %></td>
    <td><%= link_to 'Show', xxx@xxx.xxx %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Back', xxx@xxx.xxx ) %>
