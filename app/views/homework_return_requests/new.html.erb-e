<%= content_for :header do %>
	<h1>Homework Return</h1>
	for <%= @assignment.title %> of xxx@xxx.xxx %>
<% end %>

<nav>
	<h1>Navigation</h1>
	<ul>
		<li><%= link_to xxx@xxx.xxx @section, @assignment ) do %> <%= render :partial => "shared/back_link" %> <% end %></li>
	</ul>
</nav>

<section>
	<h1>Complete the Form</h1>
	<%= xxx@xxx.xxx do |f| %>
		<%= error_messages_for :homework_return_request %>
		<ol>
			<li>
				Your email address is required so that the lab you submit can be found.
				<%= f.label :email %>
				<%= f.text_field :email %>
			</li>
			<li>
				<%= f.submit "Send Request" %>
			</li>
		</ol>
	<% end %>
</section>
