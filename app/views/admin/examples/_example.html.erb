<div class="block examples">
	<header>
		<%= link_to example.file.url do %>
			<p><%= example.name %></p>
			<h5><%= show_short_date(example.created_at) %></h5>
		<% end %>
	</header>

	<ul>
		<li class="icon"><%= link_to edit_admin_semester_section_example_path(@semester,@section,example), {class: "icon icon-note", title: "Edit"} do %><span class="text_label">Edit</span><% end %></li>
		<li class="icon"><%= link_to [:admin,@semester,@section,example], {class: "icon icon-trash", title: "Destroy", data: {confirm: 'Are you sure?', method: :delete, remote: true}} do %><span class="text_label">Destroy</span><% end %></li>
	</ul>

	<p><%= example.description %></p>

</div>
