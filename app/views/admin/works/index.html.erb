
<%= render 'admin/semesters/semesters_header' %>
<%= render partial: 'admin/semesters/semester', locals: {semester: @semester} %>
<%= render partial: 'admin/courses/course', locals: {course: @section.course} %>
<%= render partial: 'admin/sections/sections_header', locals: {semester: @semester} %>
<%= render partial: @section %>
<%= render partial: 'admin/assignments/assignments_header', locals: {section: @section} %>
<%= render partial: 'admin/assignments/assignment', locals: {assignment: @assignment, main: true} %>

<%= render partial: 'work', collection: @works, cached: true %>

	
<% if @enrollments_without_works.size > 0 %>
	<% @enrollments_without_works.each do |enrollment| %>
		<%= render partial: 'admin/enrollments/enrollment', locals: {enrollment: enrollment, message: "No work for"} %>
	<% end %>
<% end %>
