<div class="block <%= double_width_if_main(defined? main) %>">
  <h3><%= division.name %></h3>
  <p><%= division.office_number %></p>
  <p><%= division.phone_number %></p>
  <p><%= division.associate_dean %></p>

  <ul>
    <li><%= link_to 'Edit', edit_admin_division_path(division) %></li>
    <li><%= link_to 'Destroy', [:admin,division], confirm: 'Are you sure?', method: :delete, remote: true %></li>
	</ul>
</div>
