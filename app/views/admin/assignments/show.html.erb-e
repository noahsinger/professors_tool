<%# admin assignments show %>

<%= render 'admin/admin/admin_header' %>
<%= render 'admin/semesters/semesters_header' %>
<%= render partial: 'admin/semesters/semester', locals: xxx@xxx.xxx %>
<%= render partial: 'admin/courses/course', locals: xxx@xxx.xxx %>
<%= render partial: 'admin/sections/sections_header', locals: xxx@xxx.xxx %>
<%= render xxx@xxx.xxx %>
<%= render partial: 'assignments_header', locals: xxx@xxx.xxx %>

<%= render partial: 'assignment', locals: xxx@xxx.xxx main: true} %>

<div class="block assignments">
	<header>
		<h3>Requirements</h3>
	</header>
	<table>
xxx@xxx.xxx do |r| %>
			<tr>
				<td><%= r.description %></td><td><%= r.points %> pts</td>
			</tr>
		<% end %>
xxx@xxx.xxx %> pts</td></tr>
	</table>
</div>

<div class="block assignments block--width2">
	<header>
		<h3>Instructions</h3>
	</header>
	<section>
		<%= xxx@xxx.xxx %>
	</section>
</div>

<% xxx@xxx.xxx > 0 %>
	<div class="block assignments">
		<header>
			<h3>Extras</h3>
		</header>
		<ul>
xxx@xxx.xxx do |extra| %>
				<li>
					<%= link_to extra.file.url do %>
						<div><%= extra.description %></div>
						<h2><%= extra.file_file_name %></h2>
						<p></p>
					<% end %>
				</li>
			<% end %>
		</ul>
	</div>
<% end %>
